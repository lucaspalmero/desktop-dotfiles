URxvt.font:                xft:DejaVu Sans Mono:pixelsize=15
URxvt.boldFont:            xft:DejaVu Sans Mono:pixelsize=15:style=Bold
URxvt.colorBD:             #FFFFFF
URxvt.cursorColor:         red
